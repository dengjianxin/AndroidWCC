<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.risen.androidwcc">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity android:name=".MainActivity" />
        <activity android:name="com.maincode.WCCBaseActivity" />
        <activity
            android:name="com.view.DialogActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.maincode.activity.SinglePageActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.maincode.index.ScrollingActivity" />
        <activity
            android:name="com.maincode.index.IndexActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>
